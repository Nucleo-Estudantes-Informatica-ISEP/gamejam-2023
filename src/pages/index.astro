---
import Container from '../layouts/Container.astro';
import Layout from '../layouts/Layout.astro';

import Hero from '../components/Hero';
import Loading from '../components/Loading';

import AwardsContainer from '../components/AwardsContainer.astro';
import ComingSoon from '../components/ComingSoon.astro';
import Footer from '../components/Footer.astro';
import JudgesContainer from '../components/JudgesContainer.astro';
import Registration from '../components/Registration.astro';
import ScheduleContainer from '../components/ScheduleContainer.astro';
import SponsorsContainer from '../components/SponsorsContainer.astro';
import WinnersContainer from '../components/WinnersContainer.astro';
---

<Layout title="Gamejam 2023">
  <Hero client:only />
</Layout>
<Container>
  <ComingSoon />
  <Registration />
  <ScheduleContainer />
  <AwardsContainer />
  <JudgesContainer />
  <SponsorsContainer />
  <WinnersContainer />
  <Loading client:only />
</Container>
<Footer />
